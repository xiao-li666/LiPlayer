   <property name="styleSheet">
    <string notr="true">/* 主列表控件样式 */
QListWidget {
    background-color: #2D2D2D;         /* 背景色 */
    border: 1px solid #3A3A3A;        /* 边框 */
    border-radius: 4px;               /* 圆角 */
    padding: 2px;                     /* 内边距 */
    outline: 0;                       /* 移除焦点虚线框 */
    font-family: &quot;Microsoft YaHei&quot;;   /* 字体 */
    font-size: 12px;                  /* 字体大小 */
    color: #CCCCCC;                   /* 默认文字颜色 */
}

/* 列表项样式 */
QListWidget::item {
    height: 28px;                     /* 项高度 */
    padding-left: 8px;                /* 左侧内边距 */
    background-color: transparent;    /* 默认透明背景 */
}

/* 鼠标悬停效果 */
QListWidget::item:hover {
    background-color: #3A3A3A;        /* 悬停背景色 */
    border-radius: 3px;
}

/* 选中项效果 */
QListWidget::item:selected {
    background-color: #4A4A4A;        /* 选中背景色 */
    color: #FFFFFF;                  /* 选中文字颜色 */
    border-left: 3px solid #1E90FF;   /* 左侧高亮条 */
}

/* 滚动条样式 */
QScrollBar:vertical {
    width: 10px;                      /* 滚动条宽度 */
    background: #2D2D2D;              /* 背景色 */
}

QScrollBar::handle:vertical {
    background: #4A4A4A;              /* 滑块颜色 */
    min-height: 20px;
    border-radius: 4px;
}

QScrollBar::handle:vertical:hover {
    background: #5A5A5A;              /* 悬停滑块颜色 */
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0px;                      /* 隐藏上下箭头 */
}</string>
